my-2 mt-5\">\n                            <v-btn color=\"#e3ba46\" to=\"/home\" fab small flat>\n                                <v-icon>mdi-close</v-icon>\n                            </v-btn>\n                        </div>\n                    </v-toolbar>\n                    <v-card-text class=\"ml-5\">\n                        Already a user?\n                        <router-link to=\"/login\">Sign in.</router-link>\n                    </v-card-text>\n                    <v-card-text class=\"ml-2\">\n                        <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n                            <v-container>\n                                <v-row dense>\n                                    <v-col cols=\"12\" md=\"6\">\n                                        <v-row dense>\n                                            <v-col cols=\"12\" md=\"6\">\n                                                <v-text-field\n                                                    v-model=\"firstname\"\n                                                    :rules=\"nameRules\"\n                                                    label=\"First name\"\n                                                    outlined\n                                                    filled\n                                                    required\n                                                ></v-text-field>\n                                            </v-col>\n\n                                            <v-col cols=\"12\" md=\"6\">\n                                                <v-text-field\n                                                    v-model=\"lastname\"\n                                                    :rules=\"nameRules\"\n                                                    label=\"Last name\"\n                                                    outlined\n                                                    filled\n                                                    required\n                                                ></v-text-field>\n                                            </v-col>\n                                        </v-row>\n                                        <v-row dense>\n                                            <v-col cols=\"12\" md=\"12\">\n                                                <v-text-field\n                                                    v-model=\"email\"\n                                                    :rules=\"emailRules\"\n                                                    label=\"E-mail\"\n                                                    outlined\n                                                    filled\n                                                    required\n                                                ></v-text-field>\n                                            </v-col>\n                                        </v-row>\n                                        <v-row dense>\n                                            <v-col cols=\"12\" md=\"12\">\n                                                <v-text-field\n                                                    name=\"password\"\n                                                    label=\"Password\"\n                                                    id=\"password\"\n                                                    type=\"password\"\n                                                    outlined\n                                                    required\n                                                    filled\n                                                    v-model=\"password\"\n                                                    :rules=\"passwordRules\"\n                                                >\n                                                </v-text-field>\n                                            </v-col>\n                                        </v-row>\n                                        <v-row dense>\n                                            <v-col cols=\"12\" md=\"12\">\n                                                <v-btn\n                                                    @click=\"submit\"\n                                                    width=\"inherit\"\n                                                    color=\"#e3ba46\"\n                                                    :disabled=\"!valid\"\n                                                    >Sign Up</v-btn\n                                                >\n                                            </v-col>\n                                        </v-row>\n                                        <v-row>\n                                            <v-col cols=\"12\" md=\"12\">\n                                                <v-divider></v-divider>\n                                            </v-col>\n                                        </v-row>\n                                        <v-row>\n                                            <v-col cols=\"12\" md=\"12\">\n                                                <div class=\"center\">\n                                                    <v-btn\n                                                        v-for=\"icon in icons\"\n                                                        :key=\"icon\"\n                                                        class=\"mx-4 center\"\n                                                        fab\n                                                        outlined\n                                                        dark\n                                                        color=\"#e3ba46\"\n                                                        icon\n                                                    >\n                                                        <v-icon size=\"24px\">\n                                                            {{ icon }}\n                                                        </v-icon>\n                                                    </v-btn>\n                                                </div>\n                                            </v-col>\n                                        </v-row>\n                                    </v-col>\n                                    <v-col cols=\"12\" md=\"6\">\n                                        <v-img\n                                            src=\"@/static/assets/images/login-img@2x.png\"\n                                            class=\"center\"\n                                        >\n                                        </v-img>\n                                    </v-col>\n                                </v-row>\n                            </v-container>\n                        </v-form>\n                    </v-card-text>\n                    <div>\n                        <v-spacer></v-spacer>\n                        <v-card-text>uBudget</v-card-text>\n                    </div>\n                </v-card>\n            </v-flex>\n        </v-layout>\n    </v-container>\n</template>\n<script>\nimport {postUser}  from '../scripts/API.js'\nexport default {\n    \n\n    name: \"Signup\",\n    data() {\n        return {\n            icons: [\n                \"mdi-facebook\",\n                \"mdi-twitter\",\n                \"mdi-linkedin\",\n                \"mdi-instagram\",\n            ],\n            valid: false,\n            firstname: \"\",\n            lastname: \"\",\n            email: \"\",\n            password: \"\",\n            nameRules: [(v) => !!v || \"Your full name is required\"],\n            emailRules: [\n                (v) => !!v || \"E-mail is required\",\n                (v) => /.+@.+/.test(v) || \"E-mail must be valid\",\n            ],\n            passwordRules: [\n                (v) => !!v || \"Password is required\",\n                (v) =>\n                    v.length >= 6 ||\n                    \"Password must be greater than 6 characters\",\n            ],\n        };\n    },\n    methods: {\n        submit() {\n            if (this.$refs.form.validate()) {\n                let full_name = this.firstname + ' ' + this.lastname\n                console.log(\"Success! Got response:/n \", postUser(full_name, this.email, this.password))               \n            }\n        },\n    },\n};\n</script>\n<style scoped>\n.signup-background {\n    background-color: #abe7ca;\n    background-size: cover;\n    width: 100%;\n    height: 100%;\n}\na {\n    font-family: \"Noto Sans\";\n    color: #e3ba46;\n    text-decoration: none;\n}\n.v-text-field {\n    margin: 0;\n    padding: 0;\n}\n.center {\n    text-align: center;\n}\n</style>\n"]}]}